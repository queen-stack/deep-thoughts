  "dependencies": {
    "if-env": "^1.0.4",
    "nodemon": "^2.0.6",
    "npm-watch": "^0.7.0"
  },
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/queen-stack/deep_thoughts.git"
  },
